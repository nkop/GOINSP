<Window x:Class="GOINSP.InspectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InspectionWindow" Height="400" Width="600"
        DataContext="{Binding Inspection, Source={StaticResource Locator}}"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid AutoGenerateColumns="False" CanUserAddRows="False" ItemsSource="{Binding Inspections}" Height="130" ColumnWidth="*" Margin="0 20 0 0">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding name}" Width="150" />
                <DataGridTextColumn Header="Longtitude" Binding="{Binding longtitude}" Width="150" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
